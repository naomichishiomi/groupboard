<% if groups.any? %>
  <% groups.each do |group| %>
    <% unless group.group_name == role.name + '-' +  "admin" %>
      <div class="col-xs-6  col-md-4">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title">
              <%= group.group_name %>
            </h3>
          </div>
          <div class="panel-body">
            
          </div>
        </div>  
      </div>
    <% end %>
  <% end %>
  <%= paginate groups %>
<% end %>